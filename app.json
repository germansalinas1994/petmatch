{
  "expo": {
    "name": "petmatch",
    "slug": "petmach",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "backgroundColor": "#ffffff",
      "resizeMode": "contain",
      "image": "./assets/images/splash.png"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.gsalinas1994.petmach"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.gsalinas1994.petmach"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "react-native-auth0",
        {
          "domain": "petmatch.us.auth0.com"
        }
      ],
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermision": "Select and upload iamges.",
          "cameraPermission": "Allow petmach to access your camera."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "abeecef0-3ba5-49fa-a2cb-1c1a67216de7"
      }
    },
    "owner": "tomaspipolo1",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/9bc6ac50-0b8d-423d-b55f-753bf8bb020c"
    }
  }
}
